{
  "name": "mindwell-mobile",
  "version": "0.1.0",
  "private": true,
  "main": "index.js",
  "scripts": {
    "start": "expo start --clear",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "lint": "eslint . --ext .ts,.tsx",
    "typecheck": "tsc --noEmit",
    "profile:android": "../../scripts/mobile-profile-android.sh",
    "optimize:assets": "npx expo optimize --quality 70 --include \"**/*.{png,jpg,jpeg}\"",
    "build:release": "expo export --output-dir build/release --platform all --dump-sourcemap"
  },
  "dependencies": {
    "@react-native-async-storage/async-storage": "~1.23.1",
    "base-64": "^1.0.0",
    "expo": "~51.0.8",
    "expo-av": "~14.0.7",
    "expo-constants": "~16.0.1",
    "expo-device": "~6.0.0",
    "expo-file-system": "~17.0.1",
    "expo-linking": "~6.3.1",
    "expo-localization": "~15.0.3",
    "expo-notifications": "~0.28.5",
    "expo-status-bar": "~2.0.0",
    "react": "18.2.0",
    "react-native": "0.74.2",
    "react-native-gesture-handler": "~2.16.1",
    "react-native-safe-area-context": "4.10.3",
    "react-native-screens": "~3.31.1",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "@babel/core": "^7.24.7",
    "@types/react": "~18.2.47",
    "@types/react-native": "~0.73.0",
    "@types/uuid": "^9.0.8",
    "eslint": "^8.57.0",
    "eslint-config-universe": "^15.0.3",
    "typescript": "^5.4.5"
  },
  "engines": {
    "node": ">=18.17.0"
  }
}
